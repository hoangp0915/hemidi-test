{
    "useTabs": false,
    "singleQuote": true,
    "tabWidth": 4,
    "printWidth": 100,
    "semi": true,
    "bracketSpacing": true,
    "trailingComma": "es5",
    "htmlWhitespaceSensitivity": "ignore",
    "overrides": [
        {
            "files": "*.html",
            "options": {
                "parser": "angular"
            }
        }
    ],
    "endOfLine": "auto"
}
